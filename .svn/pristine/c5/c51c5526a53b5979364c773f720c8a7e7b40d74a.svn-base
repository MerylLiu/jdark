@CHARSET "UTF-8";
/*==bootstrap==*/

.container-fluid {
    padding-right: 0px;
    padding-left: 0px;
}

.panel {
    padding: 0;
    margin-bottom:0;
}

.panel, .panel *, .btn {
    border-radius: 0;
    -webkit-border-radius: 0;
    -moz-border-radius: 0;
}

.btn > i {
    margin-right: 5px;
}
.nav_shortcuts .btn > i {
	margin-right:0;
}

.panel-title .fa,.wrap-collapse .fa{
    color: #bbb;
}

.panel-body {
    padding: 6px;
}

.navbar .btn, .navbar .btn-group {
    padding: 2px 12px;
    border-radius: 0;
}

.dropdown-toggle {
    text-decoration: none !important;
}

.dropdown-menu {
    position: absolute;
    top: 100%;
    left: 0;
    z-index: 1000;
    display: none;
    float: left;
    min-width: 160px;
    padding: 0;
    margin: 0;
    list-style: none;
    background-color: #fff;
    border: 1px solid #ddd;
    border-top: none;
    -webkit-border-radius: 6px;
    -moz-border-radius: 0px;
    border-radius: 0px;
    -webkit-box-shadow: 0 5px 10px rgba(0,0,0,0.2);
    -moz-box-shadow: 0 5px 10px rgba(0,0,0,0.2);
    box-shadow: 0 5px 10px rgba(0,0,0,0.2);
    -webkit-background-clip: padding-box;
    -moz-background-clip: padding;
    background-clip: padding-box;
}

.dropdown-menu .divider{
	margin:0;
}

    .dropdown-menu > li > a {
        display: block;
        padding: 3px 20px;
        clear: both;
        font-weight: normal;
        line-height: 20px;
        color: #2c3e50;
        white-space: nowrap;
    }

.navbar .pull-right > li > .dropdown-menu:after, .navbar .nav > li > .dropdown-menu.pull-right:after {
    right: 13px;
    left: auto;
}

.navbar .nav > li > .dropdown-menu:after {
    position: absolute;
    top: -6px;
    left: 10px;
    display: inline-block;
    border: none;
    content: '';
}

.nav .open > a {
    color: #fff;
    background-color: #1dd2af;
}

    .nav .open > a:focus {
        color: #fff;
        background-color: #1dd2af;
        border-color: #fafafa;
        border-color: #fafafa;
    }

.nav .open .open > a, .nav .open .open > a:focus {
    color: inherit;
    background-color: #f4f4f4;
}

.nav .open > a:hover, .nav > li > a:focus {
    border-color: #fafafa;
}

.nav > li > a:hover, .nav > li > a:focus {
    background-color: inherit;
}

.nav > .open > .submenu {
    border-left: solid 4px #1dd2af;
    background-color: #eee;
}

.modal.in .modal-dialog{-webkit-transform:translate(0,-40%);-ms-transform:translate(0,-40%);-o-transform:translate(0,-40%);transform:translate(0,-40%)}
.modal-dialog{position:absolute;width:auto;margin:10px auto;left:0;right:0;top:40%}
@media (min-width:768px){.modal-dialog{width:600px}}

.modal-body{
  max-height:400px;
  overflow-y:auto;
}

.modal-footer {
    padding: 10px 15px;
    text-align: right;
    border-top: 1px solid #e5e5e5;
}
.form-control{
	padding:0 0 0 12px;
}
.form-horizontal .radio, .form-horizontal .checkbox, .form-horizontal .radio-inline, .form-horizontal .checkbox-inline {
    padding-top: 3px;
    margin-top: 0;
    margin-bottom: 0;
}

fieldset{padding:.35em .625em .75em;margin:0 2px;border:1px solid silver;margin-bottom:.75em}
fieldset:last-child{margin-bottom:0}
legend{padding:0 .5em;border:0;width:auto;margin-bottom: 16px;}

.col-xs-1, .col-sm-1, .col-md-1, .col-lg-1, .col-xs-2, .col-sm-2, .col-md-2, 
.col-lg-2, .col-xs-3, .col-sm-3, .col-md-3, .col-lg-3, .col-xs-4, .col-sm-4, 
.col-md-4, .col-lg-4, .col-xs-5, .col-sm-5, .col-md-5, .col-lg-5, .col-xs-6, 
.col-sm-6, .col-md-6, .col-lg-6, .col-xs-7, .col-sm-7, .col-md-7, .col-lg-7, 
.col-xs-8, .col-sm-8, .col-md-8, .col-lg-8, .col-xs-9, .col-sm-9, .col-md-9, 
.col-lg-9, .col-xs-10, .col-sm-10, .col-md-10, .col-lg-10, .col-xs-11, .col-sm-11, 
.col-md-11, .col-lg-11, .col-xs-12, .col-sm-12, .col-md-12, .col-lg-12{
	padding-left:0;
}
/*==bootstrap end==*/

